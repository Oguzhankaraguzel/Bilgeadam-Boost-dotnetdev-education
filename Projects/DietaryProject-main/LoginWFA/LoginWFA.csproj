      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SettingsFormLocalization.resx</DependentUpon>
    </Compile>
    <Compile Update="SignupLocalization.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SignupLocalization.resx</DependentUpon>
    </Compile>
    <Compile Update="SummaryLocalization.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>SummaryLocalization.resx</DependentUpon>
    </Compile>
    <Compile Update="UserDetailsLocalization.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>UserDetailsLocalization.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="ActivityLocalization.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ActivityLocalization.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="CalorieLocalization.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>CalorieLocalization.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Localization.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Localization.Designer.cs</LastGenOutput>
    </EmbeddedResource>
